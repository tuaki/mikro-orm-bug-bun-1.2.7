{
    "name": "test",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "types": "bun tsc --build --verbose",
        "cli": "bun run src/cli.ts",
        "mikro-orm": "bunx --bun mikro-orm"
    },
    "mikro-orm": {
        "configPaths": [
            "./src/orm.ts"
        ],
        "useTsNode": false,
        "alwaysAllowTs": true
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not ie <= 11",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@mikro-orm/cli": "^6.4.9",
        "@mikro-orm/core": "^6.4.9",
        "@mikro-orm/migrations": "^6.4.9",
        "@mikro-orm/postgresql": "^6.4.9",
        "@mikro-orm/reflection": "^6.4.9",
        "@t3-oss/env-core": "^0.11.0",
        "commander": "^12.1.0",
        "express": "^4.19.2",
        "typescript": "^5.6.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bun": "1.2.8",
        "@types/express": "^4.17.21"
    }
}
